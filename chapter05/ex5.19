#include <iostream>
#include <string>
using namespace std;
int main()
{
	string str;
	do
	{
		string str1, str2;
		cout << "Please enter two sentences:" << endl;
		cin >> str1 >> str2;
		cout << (str1 <= str2 ? str1 : str2) << " is less than the other." << endl;
		cout << "More? Enter yes or no:" << endl;
		cin >> str;
	} while (!str.empty() && str[0] == 'y');
	return 0;
}
