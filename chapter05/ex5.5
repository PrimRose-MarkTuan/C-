#include <iostream>
#include <string>
#include <vector>
using namespace std;
int main()
{
	const vector<string> scores = {"F","D","C","B","A","A++"};
	cout << "Please enter your grade:" << endl;
	int grade = 0;
	while (cin >> grade)
	{
		if (grade < 60)
			cout << "Your score is:" << scores[0] << endl;
		else
		{
			int score = grade / 10 - 5;
			cout << "Your score is:" << scores[score] << endl;
		}
	}
	return 0;
}
