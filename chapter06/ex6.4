#include <iostream>
using namespace std;
int jiecheng(int x)
{
	int result=1;
	if (x == 0 && x == 1)
		result = 1;
	else
	{
		for (int i = x; i > 1; --i)
			result = result*i;
	}
	return result;
}
int main()
{
	cout << "Please enter a interger:" << endl;
	int a;
	cin >> a;
	int result=jiecheng(a);
	cout << "The jiecheng of " << a << " is " << result << endl;
	return 0;
}
